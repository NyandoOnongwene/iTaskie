# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# For Supabase Edge Functions (server-side operations)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Email service configuration for Supabase Edge Function
# These are server-side secrets stored in Supabase Dashboard
# SMTP_HOST=smtp.example.com
# SMTP_PORT=587
# SMTP_USERNAME=your_email@example.com
# SMTP_PASSWORD=your_password
# FROM_EMAIL=your_app@example.com

# Alternative: EmailJS Configuration
# Used as a fallback if Supabase Edge Functions cannot be deployed
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
VITE_EMAILJS_USER_ID=your_emailjs_user_id 